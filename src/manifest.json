{
  "manifest_version": 3,
  "name": "GitHub File Diff",
  "version": "1.1.10",
  "description": "View diffs of files and directories on GitHub.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png",
    "256": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["vendor.css"],
      "js": ["vendor.js", "main.js"]
    }
  ]
}
